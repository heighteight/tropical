	Title = {What Shape Is Your Conjugate? A Survey of Computational Convex Analysis and Its Applications},
	Url = {https://doi.org/10.1137/080719613},
	Volume = {20},
	Year = {2009},
	Bdsk-Url-1 = {https://doi.org/10.1137/080719613}}

